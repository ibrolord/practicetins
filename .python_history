import subprocess
proc = subprocess.run(["ls", "-l"])
proc1 = subprocess.run(
	['ls', '-l'],
	stdout=subprocess.PIPE,
	stderr=subprocess.PIPE,
)
proc
proc1
proc1.stdout
print(proc1.stdout)
bytes([46, 46, 46])
bytes([255, 1, 0])
print(proc.stdout.decode())
newp = subprocess.run('cat', 'sd'])
newp = subprocess.run('cat', 'sd.txt'])
newp = subprocess.run(['cat', 'sd.txt'])
newp
newp = subprocess.run(['cat', 'sd.txt'], check=True)
ls
